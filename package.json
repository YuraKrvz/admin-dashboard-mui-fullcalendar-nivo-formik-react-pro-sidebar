{
  "name": "admin-dashboard",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "test": "vitest",
    "build": "vite build",
    "preview": "vite preview",
    "prettier": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "prefer-const": "error",
      "no-unused-vars": "warn",
      "no-console": "warn",
      "quotes": [
        "error",
        "single"
      ],
      "jsx-quotes": [
        "error",
        "prefer-single"
      ],
      "indent": [
        "warn",
        2,
        {
          "SwitchCase": 1,
          "ignoredNodes": [
            "ConditionalExpression",
            "flatTernaryExpressions",
            "offsetTernaryExpressions"
          ]
        }
      ],
      "max-len": [
        "warn",
        {
          "code": 100
        }
      ],
      "comma-dangle": [
        "warn",
        "always-multiline"
      ],
      "semi": [
        "warn",
        "never"
      ],
      "import/order": [
        "warn",
        {
          "groups": [
            "builtin",
            "external",
            "internal",
            "parent",
            "sibling",
            "index",
            "object",
            "type"
          ],
          "newlines-between": "always-and-inside-groups"
        }
      ]
    }
  },
  "prettier": {
    "useTabs": false,
    "tabWidth": 2,
    "trailingComma": "all",
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "bracketSpacing": true,
    "arrowParens": "always",
    "jsxSingleQuote": true
  },
  "dependencies": {
    "@emotion/react": "^11.10.5",
    "@emotion/styled": "^11.10.5",
    "@fullcalendar/core": "^6.1.1",
    "@fullcalendar/daygrid": "^6.1.1",
    "@fullcalendar/interaction": "^6.1.1",
    "@fullcalendar/list": "^6.1.1",
    "@fullcalendar/react": "^6.1.2",
    "@fullcalendar/timegrid": "^6.1.1",
    "@mui/icons-material": "^5.11.0",
    "@mui/material": "^5.11.6",
    "@mui/x-data-grid": "^5.17.20",
    "@nivo/bar": "^0.80.0",
    "@nivo/core": "^0.80.0",
    "@nivo/geo": "^0.80.0",
    "@nivo/line": "^0.80.0",
    "@nivo/pie": "^0.80.0",
    "@reduxjs/toolkit": "^1.9.2",
    "axios": "^1.3.3",
    "eslint-plugin-import": "^2.27.5",
    "formik": "^2.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-pro-sidebar": "^1.0.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.7.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@vitejs/plugin-react": "^3.0.0",
    "eslint": "^8.0.0",
    "eslint-config-react-app": "^7.0.1",
    "prettier": "2.8.4",
    "vite": "^4.0.0",
    "vitest": "^0.28.5"
  }
}
